<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.main.careerbro.modules.corperation.dao.CorperationDao">

    <sql id="corperationColumns">
        a.id as id,
        c.cname as city,
        a.cname as cName,
        a.ename as eName,
        a.sname as sName
    </sql>

    <sql id="corperationJoins">
        LEFT JOIN city c ON c.id = a.city_id
    </sql>
    
    <select id="getCorperation" resultType="com.main.careerbro.modules.corperation.entity.Corperation">
        SELECT 
        <include refid="corperationColumns"/>
        FROM corperation a
        <include refid="corperationJoins"/>
        WHERE a.cname LIKE #{temp}
        AND rownum &lt; 6
    </select>


</mapper>